{
  "name": "group5-backend",
  "version": "1.0.0",
  "description": "Backend API for Group5 project",
  "main": "backend.js",
  "scripts": {
    "start": "node backend.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "express": "^4.18.2",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": ["**/test_*.js"],
    "collectCoverageFrom": [
      "*.js",
      "!**/node_modules/**"
    ]
  },
  "keywords": ["api", "express", "sqlite", "users"],
  "author": "Group5",
  "license": "MIT"
}